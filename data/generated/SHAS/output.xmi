<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="http://www.omg.org/spec/UML/20090901">
    <UML:Model name="ClassDiagram" visibility="public">
        <UML:Enumeration name="DeviceType">
            <UML:Literal name="temperatureSensor"/>
            <UML:Literal name="movementSensor"/>
            <UML:Literal name="lightController"/>
            <UML:Literal name="lockController"/>
        </UML:Enumeration>
        <UML:Enumeration name="RuleStatus">
            <UML:Literal name="active"/>
            <UML:Literal name="inactive"/>
        </UML:Enumeration>
        <UML:Enumeration name="CommandStatus">
            <UML:Literal name="requested"/>
            <UML:Literal name="completed"/>
            <UML:Literal name="failed"/>
        </UML:Enumeration>
        <UML:Enumeration name="BooleanOperator">
            <UML:Literal name="and"/>
            <UML:Literal name="or"/>
            <UML:Literal name="not"/>
        </UML:Enumeration>
        <UML:Class name="SHAS" visibility="public">
        </UML:Class>
        <UML:Class name="SmartHome" visibility="public">
            <UML:Attribute name="address" type="string"/>
        </UML:Class>
        <UML:Class name="Room" visibility="public">
            <UML:Attribute name="name" type="string"/>
        </UML:Class>
        <UML:Class name="Device" visibility="public">
            <UML:Attribute name="id" type="string"/>
            <UML:Attribute name="deviceType" type="DeviceType"/>
        </UML:Class>
        <UML:Class name="SensorReading" visibility="public">
            <UML:Attribute name="measuredValue" type="int"/>
            <UML:Attribute name="date" type="Date"/>
        </UML:Class>
        <UML:Class name="ControlCommand" visibility="public">
            <UML:Attribute name="command" type="string"/>
            <UML:Attribute name="date" type="Date"/>
            <UML:Attribute name="status" type="CommandStatus"/>
        </UML:Class>
        <UML:Class name="ActivityLog" visibility="public">
        </UML:Class>
        <UML:Class name="AutomationRule" visibility="public">
            <UML:Attribute name="operator" type="BooleanOperator"/>
            <UML:Attribute name="status" type="RuleStatus"/>
            <UML:Attribute name="action" type="string"/>
        </UML:Class>
    </UML:Model>
</XMI>