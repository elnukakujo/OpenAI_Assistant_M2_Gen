<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="http://www.omg.org/spec/UML/20090901">
    <UML:Model name="ClassDiagram" visibility="public">
        <UML:Enumeration name="Mode">
            <UML:Literal name="design"/>
            <UML:Literal name="game"/>
        </UML:Enumeration>
        <UML:Enumeration name="Color">
            <UML:Literal name="red"/>
            <UML:Literal name="blue"/>
            <UML:Literal name="green"/>
            <UML:Literal name="yellow"/>
        </UML:Enumeration>
        <UML:Class name="TileO" visibility="public">
        </UML:Class>
        <UML:Class name="Game" visibility="public">
            <UML:Attribute name="currentConnectionPieces" type="int"/>
            <UML:Attribute name="mode" type="Mode"/>
            <UML:Attribute name="spaceConnectionPieces" type="int"/>
            <UML:Attribute name="32" type="int"/>
            <UML:Attribute name="numberOfActionCards" type="int"/>
            <UML:Attribute name="32" type="int"/>
        </UML:Class>
        <UML:Class name="Connection" visibility="public">
        </UML:Class>
        <UML:Class name="WinTile" visibility="public">
        </UML:Class>
        <UML:Class name="Die" visibility="public">
        </UML:Class>
        <UML:Class name="Deck" visibility="public">
        </UML:Class>
        <UML:Class name="Tile" visibility="abstract">
            <UML:Attribute name="x" type="int"/>
            <UML:Attribute name="y" type="int"/>
        </UML:Class>
        <UML:Class name="ActionCard" visibility="abstract">
            <UML:Attribute name="instructions" type="string"/>
        </UML:Class>
        <UML:Class name="RollDieActionCard" visibility="public">
        </UML:Class>
        <UML:Class name="ConnectTilesActionCard" visibility="public">
        </UML:Class>
        <UML:Class name="RemoveConnectionActionCard" visibility="public">
        </UML:Class>
        <UML:Class name="TeleportActionCard" visibility="public">
        </UML:Class>
        <UML:Class name="LoseTurnActionCard" visibility="public">
        </UML:Class>
        <UML:Class name="WinTile" visibility="public">
        </UML:Class>
        <UML:Class name="ActionTile" visibility="public">
        </UML:Class>
        <UML:Class name="NormalTile" visibility="public">
        </UML:Class>
        <UML:Class name="Player" visibility="public">
            <UML:Attribute name="number" type="int"/>
            <UML:Attribute name="turnsUntilActive" type="int"/>
            <UML:Attribute name="color" type="Color"/>
        </UML:Class>
    </UML:Model>
</XMI>