<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="http://www.omg.org/spec/UML/20090901">
    <UML:Model name="ClassDiagram" visibility="public">
        <UML:Enumeration name="ActionCard">
            <UML:Literal name="RollDieExtraTurn"/>
            <UML:Literal name="ConnectTwoTiles"/>
            <UML:Literal name="RemoveConnectionPiece"/>
            <UML:Literal name="MoveToAnyTile"/>
            <UML:Literal name="LoseNextTurn"/>
        </UML:Enumeration>
        <UML:Class name="TileO" visibility="public">
        </UML:Class>
        <UML:Class name="Board" visibility="public">
        </UML:Class>
        <UML:Class name="Tile" visibility="public">
            <UML:Attribute name="isVisible" type="boolean"/>
            <UML:Attribute name="isStartTile" type="boolean"/>
            <UML:Attribute name="isHiddenTile" type="boolean"/>
            <UML:Attribute name="visited" type="boolean"/>
        </UML:Class>
        <UML:Class name="ConnectionPiece" visibility="public">
        </UML:Class>
        <UML:Class name="Player" visibility="public">
            <UML:Attribute name="color" type="string"/>
        </UML:Class>
        <UML:Class name="Die" visibility="public">
            <UML:Attribute name="number" type="int"/>
        </UML:Class>
        <UML:Class name="ActionTile" visibility="public">
            <UML:Attribute name="restTurns" type="int"/>
        </UML:Class>
        <UML:Class name="Deck" visibility="public">
        </UML:Class>
        <UML:Class name="ActionCard" visibility="public">
            <UML:Attribute name="action" type="ActionCard"/>
        </UML:Class>
    </UML:Model>
</XMI>