<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="http://www.omg.org/spec/UML/20090901">
    <UML:Model name="ClassDiagram" visibility="public">
        <UML:Enumeration name="RoomType">
            <UML:Literal name="single"/>
            <UML:Literal name="double"/>
            <UML:Literal name="twin"/>
        </UML:Enumeration>
        <UML:Enumeration name="PaymentType">
            <UML:Literal name="prePaid"/>
            <UML:Literal name="paidAtHotel"/>
        </UML:Enumeration>
        <UML:Class name="HBMS" visibility="public">
        </UML:Class>
        <UML:Class name="Traveler" visibility="public">
            <UML:Attribute name="name" type="string"/>
            <UML:Attribute name="billingInfo" type="string"/>
            <UML:Attribute name="preferences" type="string"/>
            <UML:Attribute name="reliabilityRating" type="double"/>
        </UML:Class>
        <UML:Class name="SearchCriteria" visibility="public">
            <UML:Attribute name="city" type="string"/>
            <UML:Attribute name="arrivalDate" type="Date"/>
            <UML:Attribute name="departureDate" type="Date"/>
            <UML:Attribute name="roomsNeeded" type="int"/>
            <UML:Attribute name="roomType" type="RoomType"/>
            <UML:Attribute name="minHotelRating" type="double"/>
            <UML:Attribute name="budget" type="double"/>
            <UML:Attribute name="preferences" type="string"/>
        </UML:Class>
        <UML:Class name="Booking" visibility="public">
            <UML:Attribute name="price" type="double"/>
            <UML:Attribute name="area" type="string"/>
            <UML:Attribute name="hotelRating" type="double"/>
            <UML:Attribute name="preferences" type="string"/>
            <UML:Attribute name="bookingID" type="string"/>
            <UML:Attribute name="payment" type="PaymentType"/>
            <UML:Attribute name="confirmationDeadline" type="Date"/>
            <UML:Attribute name="cancellationDeadline" type="Date"/>
        </UML:Class>
        <UML:Class name="Hotel" visibility="public">
            <UML:Attribute name="name" type="string"/>
            <UML:Attribute name="address" type="string"/>
            <UML:Attribute name="city" type="string"/>
            <UML:Attribute name="rating" type="double"/>
            <UML:Attribute name="roomInfo" type="string"/>
        </UML:Class>
        <UML:Class name="Offer" visibility="public">
            <UML:Attribute name="price" type="double"/>
        </UML:Class>
    </UML:Model>
</XMI>