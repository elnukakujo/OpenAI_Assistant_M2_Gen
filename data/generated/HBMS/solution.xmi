<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="http://www.omg.org/spec/UML/20090901">
    <UML:Model name="ClassDiagram" visibility="public">
        <UML:Enumeration name="RoomType">
            <UML:Literal name="SINGLE"/>
            <UML:Literal name="DOUBLE"/>
            <UML:Literal name="TWIN"/>
        </UML:Enumeration>
        <UML:Enumeration name="BookingStatus">
            <UML:Literal name="FINALIZED"/>
            <UML:Literal name="CONFIRMED"/>
            <UML:Literal name="CANCELLED_BY_HBMS"/>
            <UML:Literal name="PRELIMINARY"/>
            <UML:Literal name="CANCELLED_BY_TRAVELLER"/>
            <UML:Literal name="CANCELLED_BY_HOTEL"/>
        </UML:Enumeration>
        <UML:Enumeration name="PaymentType">
            <UML:Literal name="PREPAID"/>
            <UML:Literal name="PAID_AT_HOTEL"/>
        </UML:Enumeration>
        <UML:Class name="HotelChain" visibility="public">
        </UML:Class>
        <UML:Class name="HotelBookingManager" visibility="public">
        </UML:Class>
        <UML:Class name="Traveller" visibility="public">
            <UML:Attribute name="name" type="string"/>
            <UML:Attribute name="reliabilityRating" type="int"/>
        </UML:Class>
        <UML:Class name="BillingInformation" visibility="public">
            <UML:Attribute name="companyName" type="string"/>
            <UML:Attribute name="address" type="string"/>
        </UML:Class>
        <UML:Class name="TravelPreference" visibility="public">
            <UML:Attribute name="preference" type="string"/>
            <UML:Attribute name="value" type="string"/>
        </UML:Class>
        <UML:Class name="Hotel" visibility="public">
            <UML:Attribute name="city" type="string"/>
            <UML:Attribute name="country" type="string"/>
            <UML:Attribute name="area" type="string"/>
            <UML:Attribute name="availability" type="RoomAvailability"/>
        </UML:Class>
        <UML:Class name="Offer" visibility="abstract">
        </UML:Class>
        <UML:Class name="SpecialOffer" visibility="public">
        </UML:Class>
        <UML:Class name="RegularOffer" visibility="public">
        </UML:Class>
        <UML:Class name="TravelSearch" visibility="public">
        </UML:Class>
        <UML:Class name="SearchInfo" visibility="public">
        </UML:Class>
        <UML:Class name="RoomAvailability" visibility="public">
        </UML:Class>
        <UML:Class name="TripInfo" visibility="abstract">
            <UML:Attribute name="numberOfRooms" type="int"/>
            <UML:Attribute name="roomType" type="RoomType"/>
            <UML:Attribute name="arrivalDate" type="date"/>
            <UML:Attribute name="departureDate" type="date"/>
            <UML:Attribute name="budgetPerNight" type="float"/>
        </UML:Class>
        <UML:Class name="BookingInfo" visibility="public">
        </UML:Class>
        <UML:Class name="Booking" visibility="public">
            <UML:Attribute name="bookingId" type="int"/>
            <UML:Attribute name="cancellationDeadline" type="date"/>
            <UML:Attribute name="creditCardNumber" type="string"/>
            <UML:Attribute name="bookingStatus" type="BookingStatus"/>
            <UML:Attribute name="paymentType" type="PaymentType"/>
            <UML:Attribute name="confirmationDate" type="date"/>
            <UML:Attribute name="paidAmount" type="float"/>
        </UML:Class>
    </UML:Model>
</XMI>