<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="http://www.omg.org/spec/UML/20090901">
    <UML:Model name="ClassDiagram" visibility="public">
        <UML:Enumeration name="ActionCard">
            <UML:Literal name="ExtraTurn"/>
            <UML:Literal name="ConnectTiles"/>
            <UML:Literal name="RemoveConnection"/>
            <UML:Literal name="MoveToTile"/>
            <UML:Literal name="LoseTurn"/>
        </UML:Enumeration>
        <UML:Enumeration name="Color">
            <UML:Literal name="Red"/>
            <UML:Literal name="Blue"/>
            <UML:Literal name="Yellow"/>
            <UML:Literal name="Green"/>
        </UML:Enumeration>
        <UML:Class name="TileO" visibility="public">
        </UML:Class>
        <UML:Class name="Game" visibility="public">
        </UML:Class>
        <UML:Class name="Tile" visibility="public">
            <UML:Attribute name="id" type="string"/>
            <UML:Attribute name="isHidden" type="boolean"/>
            <UML:Attribute name="wasVisited" type="boolean"/>
            <UML:Attribute name="isActionTile" type="boolean"/>
            <UML:Attribute name="disableForTurns" type="int"/>
        </UML:Class>
        <UML:Class name="ConnectionPiece" visibility="public">
            <UML:Attribute name="id" type="string"/>
            <UML:Attribute name="inPile" type="bool"/>
        </UML:Class>
        <UML:Class name="Player" visibility="public">
            <UML:Attribute name="color" type="Color"/>
        </UML:Class>
        <UML:Class name="PlayingPiece" visibility="public">
            <UML:Attribute name="color" type="Color"/>
        </UML:Class>
        <UML:Class name="ActionDeck" visibility="public">
        </UML:Class>
        <UML:Class name="Die" visibility="public">
            <UML:Attribute name="faceUp" type="int"/>
        </UML:Class>
    </UML:Model>
</XMI>