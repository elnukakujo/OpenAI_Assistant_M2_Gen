<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="2.1" xmlns:UML="http://www.omg.org/spec/UML/20090901">
    <UML:Model name="ClassDiagram" visibility="public">
        <UML:Enumeration name="DeviceType">
            <UML:Literal name="sensor"/>
            <UML:Literal name="actuator"/>
        </UML:Enumeration>
        <UML:Enumeration name="Status">
            <UML:Literal name="requested"/>
            <UML:Literal name="completed"/>
            <UML:Literal name="failed"/>
        </UML:Enumeration>
        <UML:Enumeration name="Operator">
            <UML:Literal name="AND"/>
            <UML:Literal name="OR"/>
            <UML:Literal name="NOT"/>
        </UML:Enumeration>
        <UML:Class name="SHAS" visibility="public">
        </UML:Class>
        <UML:Class name="SmartHome" visibility="public">
            <UML:Attribute name="address" type="string"/>
        </UML:Class>
        <UML:Class name="Room" visibility="public">
            <UML:Attribute name="name" type="string"/>
        </UML:Class>
        <UML:Class name="Device" visibility="public">
            <UML:Attribute name="deviceId" type="string"/>
            <UML:Attribute name="type" type="DeviceType"/>
            <UML:Attribute name="isActive" type="boolean"/>
        </UML:Class>
        <UML:Class name="SensorReading" visibility="public">
            <UML:Attribute name="measuredValue" type="string"/>
            <UML:Attribute name="timestamp" type="Date"/>
        </UML:Class>
        <UML:Class name="ControlCommand" visibility="public">
            <UML:Attribute name="command" type="string"/>
            <UML:Attribute name="status" type="Status"/>
            <UML:Attribute name="timestamp" type="Date"/>
        </UML:Class>
        <UML:Class name="AutomationRule" visibility="public">
            <UML:Attribute name="operator" type="Operator"/>
            <UML:Attribute name="precondition" type="string"/>
            <UML:Attribute name="action" type="string"/>
        </UML:Class>
        <UML:Class name="ActivityLog" visibility="public">
        </UML:Class>
        <UML:Class name="RuleHierarchy" visibility="public">
        </UML:Class>
    </UML:Model>
</XMI>